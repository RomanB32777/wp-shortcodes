{
	"extends": [
		"plugin:@wordpress/eslint-plugin/recommended-with-formatting",
		"prettier"
	],
	"rules": {
		"@wordpress/no-global-event-listener": "off",
		"no-shadow": [2, { "allow": ["done"] }]
	},
	"parserOptions": {
		"ecmaVersion": "latest"
	},
	"env": {
		"browser": true,
		"es6": true
	}
}
